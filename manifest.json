{
    "manifest_version": 2,
    "name": "Page Vocab", 
    "icons": {},
    "version": "0.0.1",
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css":["main/resources/word.css"],
        "js": ["main/js/content.js", "main/jquery-3.5.1.min.js"]
      }
    ],
    "background": { "scripts": ["main/js/background.js"],  "persistent": false },
     "browser_action": {
      "icons": {},
      "default_popup": "main/resources/popup.html"
    }
  
}
